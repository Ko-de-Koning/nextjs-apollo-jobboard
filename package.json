{
  "name": "with-typescript-styled-components",
  "version": "1.0.0",
  "scripts": {
    "dev": "nodemon",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "schema:download": "apollo schema:download --endpoint=http://localhost:4000",
    "codegen:generate": "apollo codegen:generate --queries=./web/**/*.tsx --localSchemaFile=./schema.json --outputFlat --target=typescript ./schemaTypes.ts",
    "gen:types": "yarn run schema:download && yarn run codegen:generate"
  },
  "dependencies": {
    "apollo-boost": "^0.4.2",
    "apollo-link-context": "^1.0.17",
    "cookie": "^0.4.0",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "formik": "^1.5.7",
    "graphql": "^14.3.1",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^8.1.0",
    "react": "^16.8.6",
    "react-apollo": "^2.5.6",
    "react-apollo-hooks": "^0.4.5",
    "react-dom": "^16.8.6",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@types/cookie": "^0.3.3",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.17.0",
    "@types/next": "^8.0.1",
    "@types/react": "^16.8.6",
    "@types/styled-components": "^4.1.11",
    "@zeit/next-typescript": "^1.1.1",
    "babel-plugin-styled-components": "^1.10.0",
    "cross-env": "^5.2.0",
    "nodemon": "^1.19.1",
    "ts-node": "^8.2.0",
    "typescript": "^3.5.1"
  },
  "license": "MIT"
}
